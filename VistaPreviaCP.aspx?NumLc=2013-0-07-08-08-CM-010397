        }


        function validarAdquisicion() {
            var NumLC = trim(document.getElementById('ctl00_ContentPlaceHolder1_txtNumeroAdquisicion').value);
            var divMsg = document.getElementById('divNumLC');

            document.getElementById('ctl00_ContentPlaceHolder1_txtNumeroAdquisicion').value = NumLC;


            divMsg.style.display = '';

            if (NumLC == '') {
                divMsg.innerHTML = sImagenAdvertencia + ' <span class=formCamposError>Debe ingresar un número de adquisición</span>';
                return;
            }
            if (!esNumDocValido(NumLC)) {
                divMsg.innerHTML = sImagenAdvertencia + ' <span class=formCamposError>El formato del número de adquisición no es válido</span>';
                return;
            }
            divMsg.style.display = 'none';
            //window.location.href='?NumLC=' + NumLC
            //__doPostBack('btnVerAdquisicion',NumLC)

            __doPostBack('ctl00$ContentPlaceHolder1$linkBuscarNumLc', '');
        }
