            document.getElementById('ctl00_ContentPlaceHolder1_txtNombreProveedor').value = '';
            document.getElementById('ctl00_ContentPlaceHolder1_hidNombreProveedor').value = '-1';
            document.getElementById('ctl00_ContentPlaceHolder1_hidIdEmpresaVenta').value = 0;
        }

