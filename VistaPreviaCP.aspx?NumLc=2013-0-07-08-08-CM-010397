        //Funci&#65533;n de Fecha y Hora
        var months = new Array("Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio", "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre");
        var dayName = new Array("Domingo", "Lunes", "Martes", "Miércoles", "Jueves", "Viernes", "Sábado");
        var relojGlobal;
        var intDiferencia;

        function reloj() {
            var date = relojGlobal;
            intDiferencia = (relojGlobal - new Date()) / 1000;
            correrReloj();
        }
        function correrReloj() {

            //
            var difMax = 60;
            var difSeg = (relojGlobal - new Date()) / 1000;
            if (difSeg > intDiferencia + difMax || difSeg < intDiferencia - difMax)
                window.location.reload();

            var date = new Date();
            relojGlobal = new Date(date.getFullYear(), date.getMonth(), date.getDate(), date.getHours(), date.getMinutes(), date.getSeconds() + intDiferencia);
            date = relojGlobal;

            var year = date.getFullYear();
            var month = date.getMonth();
            var day = date.getDate();
            var hour = date.getHours();
            var minute = date.getMinutes();
            var second = date.getSeconds();
            var monthname = months[month];
            var AM_PM;
            if (hour > 12) {
                //hour = hour - 12;
                AM_PM = "";
            }
            else {
                AM_PM = "";
            }
            if (minute < 10) { minute = "0" + minute; }
            if (second < 10) { second = "0" + second; }
            if (month < 10) { month = "0" + month; }
            var dayString = dayName[date.getDay()];
            if (document.getElementById('divReloj') != null) {
                document.getElementById('divReloj').innerHTML = " " + dayString + " " + day + " de " + monthname + ", <b>" + hour + ":" + minute + ":" + second + " " + AM_PM + "</b>";
            }
            setTimeout("correrReloj()", 1000);
        }
        //Fin Funci&#65533;n de Fecha y Hora

        function cierraSession() {
            window.location.href = 'logoff.aspx';
        }
 
    </script>

    <script type="text/javascript">


        var gbltipo = '';
        //  Vamos a presuponer que el usuario es una persona inteligente...
        var isIE = false;
        //  Creamos una variable para el objeto XMLHttpRequest
        var req;
        //  Creamos una funcion para cargar los datos en nuestro objeto.
        //  Logicamente, antes tenemos que crear el objeto.
        //  Vease que la sintaxis varia dependiendo de si usamos un navegador decente
        //  o Internet Explorer
        function cargaXML(url, tipo) {
            //  Primero vamos a ver si la URL es una URL :)
            gbltipo = tipo;
            if (url == '') {
                return;
            }
            //  Usuario inteligente...
            if (window.XMLHttpRequest) {
                req = new XMLHttpRequest();
                req.onreadystatechange = processReqChange;
                req.open("GET", url, true);
                req.send(null);
                //  ...y usuario de Internet Explorer Windows
            } else if (window.ActiveXObject) {
                isIE = true;
                req = new ActiveXObject("Microsoft.XMLHTTP");
                if (req) {
                    req.onreadystatechange = processReqChange;
                    req.open("GET", url, true);
                    req.setRequestHeader('Content-Type', 'text/xml; charset=utf-8'); //jcadd
                    //req.setRequestHeader('Content-Type', 'application/x-www-form-rulencoded');//jcadd
                    req.send();
                }
            }
        }

        function processReqChange() {
            //    Referencia a nuestro DIV con ID unica:
            var detalles = document.getElementById("detalles");
            //    Si se ha completado la carga de datos, los mostramos en el DIV...
            if (req.readyState == 4) {

                var str = req.responseText;
                /*
                alert(str);
                while(str.indexOf('\"')>-1){
                var i = str.indexOf('\"');
                str   = str.substring(i-1) + str.substring(i+1);
                }
                alert(str);
                */
                if (gbltipo == 'pc') {
                    str = str.substring(str.indexOf('<div id=\"gralContenedor\">'));
                    str = str.substring(0, str.indexOf('</form>') - 1);
                }

                if (gbltipo == 'lc') {
                    str = str.substring(str.indexOf('<div class="gralSubtitulos">'));

                    str = str.replace('VersionAdquisicion.aspx?IdLC=', '../Adquisicion/Visualizacion/VersionAdquisicion.aspx?IdLC=');

                    str = str.substring(0, str.indexOf('</body>') - 1);

                    //VersionAdquisicion.aspx?IdLC=
                }

                if (gbltipo == 'ctr') {
                    str = str.substring(str.indexOf('<div id=\"gralContenedor\">'));

                    //str = str.replace ('VersionAdquisicion.aspx?IdLC=','../Adquisicion/Visualizacion/VersionAdquisicion.aspx?IdLC=');

                    str = str.substring(0, str.indexOf('</body>') - 1);

                    //VersionAdquisicion.aspx?IdLC=
                }


                detalles.innerHTML = str;
                //detalles.innerHTML = req.responseText;
            } else {
                //    ...en caso contrario, le diremos al usuario que los estamos cargando:
                detalles.innerHTML = 'Cargando...';
                //<img src="loading.gif" align="absmiddle" /> 
            }
        }

        function veritem(tipo, id) {
            window.location.href = 'AP_BuscarItem.aspx?tipo=' + tipo + '&id=' + id;
        }

        function Ver(Id, Tipo) {
            if (Tipo == 'Contrato')
            //window.location.href='../Contrato/VistaPreliminarPublica.aspx?Id=' + Id;
                veritem('ctr', Id);

            else if (Tipo == 'Licitacion')
                window.location.href = '../Adquisicion/Visualizacion/VistaPrevia.aspx?IdLC=' + Id;
            else if (Tipo == 'LicitacionPub')
            //window.location.href='../Adquisicion/Visualizacion/VistaPreviaPublica.aspx?IdLC=' + Id;
                veritem('lc', Id);

        }

