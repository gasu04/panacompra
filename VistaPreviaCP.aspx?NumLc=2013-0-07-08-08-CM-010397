        function CajaTT(str) {
            return '<u>' + str + '</u><br>';
        }
        function doTooltip(e, msg) {
            if (typeof Tooltip == "undefined" || !Tooltip.ready) return;
            Tooltip.clearTimer();
            Tooltip.show(e, msg);
        }

        function hideTip() {
            if (typeof Tooltip == "undefined" || !Tooltip.ready) return;
            Tooltip.timerId = setTimeout("Tooltip.hide()", 300);
        }


        var tt1 = CajaTT('¿Qué es un archivo Comprimido?') + 'Es un archivo que es resultado de un proceso de "compresión" de la información digital, a través del cual se le disminuye el peso o tamaño al archivo original para reducir su tiempo de descarga.';

