        function habilitarRegiones(idPais) {
            var lstRegion = document.getElementById('ctl00_ContentPlaceHolder1_lstRegion');
            var lstcomuna = document.getElementById('ctl00_ContentPlaceHolder1_lstMunicipio');
            if (idPais == 'PA') {
                lstRegion.disabled = false;
            }
            else {
                lstRegion.disabled = true;
                lstMunicipio.disabled = true;
                lstMunicipio.selectedIndex = 0;
                lstRegion.selectedIndex = 0;
            }
        }

        function CargaDepartamento(lstDepto) {
            //alert("entro lstDepto= " + lstDepto);
            var ddComuna = document.getElementById(lstDepto);
            var estado = ddComuna.disabled;
            ddComuna.options.length = 0;
            ddComuna.options[0] = new Option('Todos los Departamentos...', '0');
            for (i = 0; i &lt; arrRegionList[0].length; i++) {
                ddComuna.options[i + 1] = new Option(arrRegionList[0][i][0], arrRegionList[0][i][1]);
            }
            ddComuna.disabled = estado;
        }

        function listarComunaPorRegion(idRegion, lstComuna) {
            var ddComuna = document.getElementById(lstComuna);
            if (idRegion != 0) {
                ddComuna.options.length = 0;
                ddComuna.options[0] = new Option('Seleccionar Distrito...', '0');
                for (i = 0; i &lt; arrRegion[idRegion].length; i++) {
                    ddComuna.options[i + 1] = new Option(arrRegion[idRegion][i][1], arrRegion[idRegion][i][0]);
                }
                ddComuna.disabled = false;
            }
            else {
                ddComuna.value = 0;
                ddComuna.disabled = true;
            }
        }



