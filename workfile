        function PressTeclaEnter() {
            if (event.keyCode == 13) {
                event.returnValue = false;
                event.cancel = true;
                Buscar();
            }
        }
        function Buscar() {

            var blnPaginaValida = esPaginaValida();
            MSG_FORMATO_NO_VALIDO = "La fecha ingresada no es válida, por favor ingrese una fecha con el formato dd-MM-yyyy";
            MSG_DIFERENCIA_NO_VALIDA = "La fecha HASTA debe ser mayor a la fecha DESDE";
            MSG_RANGO_NO_VALIDA = "El rango de la fecha no puede ser mayor a " + document.getElementById('ctl00_ContentPlaceHolder1_hidRangoMaximoFecha').value + " días";

            FechaDesde = document.getElementById('ctl00_ContentPlaceHolder1_txtFechaDesde');
            FechaHasta = document.getElementById('ctl00_ContentPlaceHolder1_txtFechaHasta');

            //Valida el formato de la fecha Desde
            if (!(ValidarFecha(FechaDesde.value))) {
                FechaDesde.focus();
                alert(MSG_FORMATO_NO_VALIDO);
                return false;
            }

            //Valida el formato de la fecha Hasta						
            if (!(ValidarFecha(FechaHasta.value))) {
                FechaHasta.focus();
                alert(MSG_FORMATO_NO_VALIDO);
                return false;
            }

            //Valida que la fecha hasta sea mayor a la fecha desde
            if (CompararFechas(FechaDesde.value, FechaHasta.value) &gt; 0) {
                FechaHasta.focus();
                alert(MSG_DIFERENCIA_NO_VALIDA);
                return false;
            }

            if (esPaginaValida()) {
                __doPostBack('ctl00$ContentPlaceHolder1$btnBuscar', '');
            }


        }

