        var gbltipo = '';
        //  Vamos a presuponer que el usuario es una persona inteligente...
        var isIE = false;
        //  Creamos una variable para el objeto XMLHttpRequest
        var req;
        //  Creamos una funcion para cargar los datos en nuestro objeto.
        //  Logicamente, antes tenemos que crear el objeto.
        //  Vease que la sintaxis varia dependiendo de si usamos un navegador decente
        //  o Internet Explorer
        function cargaXML(url, tipo) {
            //  Primero vamos a ver si la URL es una URL :)
            gbltipo = tipo;
            if (url == '') {
                return;
            }
            //  Usuario inteligente...
            if (window.XMLHttpRequest) {
                req = new XMLHttpRequest();
                req.onreadystatechange = processReqChange;
                req.open("GET", url, true);
                req.send(null);
                //  ...y usuario de Internet Explorer Windows
            } else if (window.ActiveXObject) {
                isIE = true;
                req = new ActiveXObject("Microsoft.XMLHTTP");
                if (req) {
                    req.onreadystatechange = processReqChange;
                    req.open("GET", url, true);
                    req.setRequestHeader('Content-Type', 'text/xml; charset=utf-8'); //jcadd
                    //req.setRequestHeader('Content-Type', 'application/x-www-form-rulencoded');//jcadd
                    req.send();
                }
            }
        }

        function processReqChange() {
            //    Referencia a nuestro DIV con ID unica:
            var detalles = document.getElementById("detalles");
            //    Si se ha completado la carga de datos, los mostramos en el DIV...
            if (req.readyState == 4) {

                var str = req.responseText;
                /*
                alert(str);
                while(str.indexOf('\"')&gt;-1){
                var i = str.indexOf('\"');
                str   = str.substring(i-1) + str.substring(i+1);
                }
                alert(str);
                */
                if (gbltipo == 'pc') {
                    str = str.substring(str.indexOf('&lt;div id=\"gralContenedor\"&gt;'));
                    str = str.substring(0, str.indexOf('&lt;/form&gt;') - 1);
                }

                if (gbltipo == 'lc') {
                    str = str.substring(str.indexOf('&lt;div class="gralSubtitulos"&gt;'));

                    str = str.replace('VersionAdquisicion.aspx?IdLC=', '../Adquisicion/Visualizacion/VersionAdquisicion.aspx?IdLC=');

                    str = str.substring(0, str.indexOf('&lt;/body&gt;') - 1);

                    //VersionAdquisicion.aspx?IdLC=
                }

                if (gbltipo == 'ctr') {
                    str = str.substring(str.indexOf('&lt;div id=\"gralContenedor\"&gt;'));

                    //str = str.replace ('VersionAdquisicion.aspx?IdLC=','../Adquisicion/Visualizacion/VersionAdquisicion.aspx?IdLC=');

                    str = str.substring(0, str.indexOf('&lt;/body&gt;') - 1);

                    //VersionAdquisicion.aspx?IdLC=
                }


                detalles.innerHTML = str;
                //detalles.innerHTML = req.responseText;
            } else {
                //    ...en caso contrario, le diremos al usuario que los estamos cargando:
                detalles.innerHTML = 'Cargando...';
                //&lt;img src="loading.gif" align="absmiddle" /&gt; 
            }
        }

        function veritem(tipo, id) {
            window.location.href = 'AP_BuscarItem.aspx?tipo=' + tipo + '&amp;id=' + id;
        }

        function Ver(Id, Tipo) {
            if (Tipo == 'Contrato')
            //window.location.href='../Contrato/VistaPreliminarPublica.aspx?Id=' + Id;
                veritem('ctr', Id);

            else if (Tipo == 'Licitacion')
                window.location.href = '../Adquisicion/Visualizacion/VistaPrevia.aspx?IdLC=' + Id;
            else if (Tipo == 'LicitacionPub')
            //window.location.href='../Adquisicion/Visualizacion/VistaPreviaPublica.aspx?IdLC=' + Id;
                veritem('lc', Id);

        }

